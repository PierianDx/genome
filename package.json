{
  "name": "genome",
  "version": "0.0.0-development",
  "description": "React components for PierianDx",
  "main": "dist",
  "scripts": {
    "test": "jest",
    "test:cov": "eslint --ext .js,.jsx ./src && jest --coverage",
    "eslint": "eslint --ext .js,.jsx ./src",
    "report:coverage": "npm run test:cov && npm run coveralls",
    "coveralls": "cat ./.coverage/lcov.info | coveralls",
    "clean:build": "rm -rf dist && rm -rf lib && rm -rf full",
    "prepare": "npm run clean:build && npm run build && npm run clean:test",
    "build": "WEBPACK_ENV=INCLUDE_CSS webpack --progress --color --mode=production && webpack --progress --color --mode=production && npm run create:lib",
    "create:lib": "babel ./src --out-dir ./lib --copy-files --source-maps",
    "clean:test": "rm -rf $(find ./lib -name \"__test__\")",
    "commit": "git-cz",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PierianDx/genome.git"
  },
  "keywords": [
    "react",
    "css",
    "sass"
  ],
  "author": "David Atchley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PierianDx/genome/issues"
  },
  "homepage": "https://github.com/PierianDx/genome#readme",
  "dependencies": {
    "bulma": "0.7.4",
    "classnames": "2.2.6"
  },
  "peerDependencies": {
    "prop-types": ">=15.6.0",
    "react": "16.3.0",
    "react-dom": "16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt": "^7.5.0",
    "@emotion/core": "^10.0.9",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "babel-plugin-react-intl": "3.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-runtime": "6.26.0",
    "commitizen": "^3.0.7",
    "coveralls": "3.0.3",
    "css-loader": "2.1.0",
    "docz": "^0.13.7",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^0.13.7",
    "emotion-theming": "^10.0.9",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.14.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.3.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "jsdom": "13.2.0",
    "mini-css-extract-plugin": "0.5.0",
    "moment": "2.24.0",
    "node-sass": "4.11.0",
    "null-loader": "0.1.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "^1.16.4",
    "progress-bar-webpack-plugin": "1.12.1",
    "prop-types": "15.7.2",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-test-renderer": "16.8.3",
    "resolve-url-loader": "3.0.1",
    "sass-loader": "7.1.0",
    "semantic-release": "^15.13.3",
    "source-map-support": "0.5.10",
    "style-loader": "0.23.1",
    "toolbox-loader": "0.0.3",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.0.4",
    "webpack-cli": "3.2.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!bin/**"
    ]
  }
}
